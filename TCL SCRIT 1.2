local Tab2 = Window:NewTab("Powers")
local Section2 = Tab2:NewSection("Abilities")

Section2:NewKeybind("Source FourceField", "KeybindInfo", Enum.KeyCode.M, function(state)
local Character_upvr = game.Players.LocalPlayer.Character
local PowerEvent_upvr = game:GetService("ReplicatedStorage").PowerEvents.Source.PowerEvent
PowerEvent_upvr:FireServer("Scream", Character_upvr)

if state then
	PowerEvent_upvr:FireServer("Forcefield", Character_upvr)
else
	 PowerEvent_upvr:FireServer("ForcefieldEnd", Character_upvr)
end
end)


Section2:NewKeybind("Flaming", "KeybindInfo", Enum.KeyCode.Q, function()
  local LocalPlayer_upvr = game:GetService("Players").LocalPlayer
  local mouse_upvr = LocalPlayer_upvr:GetMouse()
  local Character = LocalPlayer_upvr.Character
  local PowerEvent_upvr = game:GetService("ReplicatedStorage"):WaitForChild("PowerEvents"):WaitForChild("EvilPhoebe"):WaitForChild("PowerEvent")
  if true and not Character:FindFirstChild("Powerless") and not Character:GetAttribute("Enchantress") then
     PowerEvent_upvr:FireServer("Flaming", Character, mouse_upvr.Hit.Position)
    return
end
end)

